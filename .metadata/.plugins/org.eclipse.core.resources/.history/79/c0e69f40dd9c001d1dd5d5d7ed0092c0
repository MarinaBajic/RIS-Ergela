package model;

import java.io.Serializable;
import javax.persistence.*;
import java.util.List;


/**
 * The persistent class for the ergela_uloga database table.
 * 
 */
@Entity
@Table(name="ergela_uloga")
@NamedQuery(name="ErgelaUloga.findAll", query="SELECT e FROM ErgelaUloga e")
public class ErgelaUloga implements Serializable {
	private static final long serialVersionUID = 1L;

	@Id
	@GeneratedValue(strategy=GenerationType.IDENTITY)
	private int idUloga;

	private String naziv;

	//bi-directional many-to-one association to ErgelaKorisnikUloga
	@OneToMany(mappedBy="ergelaUloga", fetch=FetchType.EAGER)
	private List<ErgelaKorisnikUloga> ergelaKorisnikUlogas;

	public ErgelaUloga() {
	}

	public int getIdUloga() {
		return this.idUloga;
	}

	public void setIdUloga(int idUloga) {
		this.idUloga = idUloga;
	}

	public String getNaziv() {
		return this.naziv;
	}

	public void setNaziv(String naziv) {
		this.naziv = naziv;
	}

	public List<ErgelaKorisnikUloga> getErgelaKorisnikUlogas() {
		return this.ergelaKorisnikUlogas;
	}

	public void setErgelaKorisnikUlogas(List<ErgelaKorisnikUloga> ergelaKorisnikUlogas) {
		this.ergelaKorisnikUlogas = ergelaKorisnikUlogas;
	}

	public ErgelaKorisnikUloga addErgelaKorisnikUloga(ErgelaKorisnikUloga ergelaKorisnikUloga) {
		getErgelaKorisnikUlogas().add(ergelaKorisnikUloga);
		ergelaKorisnikUloga.setErgelaUloga(this);

		return ergelaKorisnikUloga;
	}

	public ErgelaKorisnikUloga removeErgelaKorisnikUloga(ErgelaKorisnikUloga ergelaKorisnikUloga) {
		getErgelaKorisnikUlogas().remove(ergelaKorisnikUloga);
		ergelaKorisnikUloga.setErgelaUloga(null);

		return ergelaKorisnikUloga;
	}

}