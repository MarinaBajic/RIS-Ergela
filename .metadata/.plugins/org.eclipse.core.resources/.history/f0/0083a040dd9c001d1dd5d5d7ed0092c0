package model;

import java.io.Serializable;
import javax.persistence.*;


/**
 * The persistent class for the ergela_korisnik_uloga database table.
 * 
 */
@Entity
@Table(name="ergela_korisnik_uloga")
@NamedQuery(name="ErgelaKorisnikUloga.findAll", query="SELECT e FROM ErgelaKorisnikUloga e")
public class ErgelaKorisnikUloga implements Serializable {
	private static final long serialVersionUID = 1L;

	@Id
	@GeneratedValue(strategy=GenerationType.IDENTITY)
	private int idKorisnikUloga;

	//bi-directional many-to-one association to ErgelaKorisnik
	@ManyToOne
	@JoinColumn(name="idKorisnik")
	private ErgelaKorisnik ergelaKorisnik;

	//bi-directional many-to-one association to ErgelaUloga
	@ManyToOne
	@JoinColumn(name="idUloga")
	private ErgelaUloga ergelaUloga;

	public ErgelaKorisnikUloga() {
	}

	public int getIdKorisnikUloga() {
		return this.idKorisnikUloga;
	}

	public void setIdKorisnikUloga(int idKorisnikUloga) {
		this.idKorisnikUloga = idKorisnikUloga;
	}

	public ErgelaKorisnik getErgelaKorisnik() {
		return this.ergelaKorisnik;
	}

	public void setErgelaKorisnik(ErgelaKorisnik ergelaKorisnik) {
		this.ergelaKorisnik = ergelaKorisnik;
	}

	public ErgelaUloga getErgelaUloga() {
		return this.ergelaUloga;
	}

	public void setErgelaUloga(ErgelaUloga ergelaUloga) {
		this.ergelaUloga = ergelaUloga;
	}

}